{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "types": ["vue", "@nuxt/content"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "#content": ["node_modules/@nuxt/content/dist/runtime"],
      "#imports": ["./.nuxt/imports"]
    }
  }
}

